{
  "name": "admin-frontend-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {},
  "husky": {
    "hooks": {
      "pre-commit": "npx --no-install lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": "make precommit-ts",
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix --quiet",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --quiet",
      "git add"
    ],
    "*.{json,scss,css,md,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.8.0",
    "@ant-design/pro-components": "^2.4.12",
    "@tanstack/react-query": "^4.24.9",
    "antd": "5.1.6",
    "axios": "1.2.1",
    "bigfloat.js": "^3.0.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "json5": "^2.2.3",
    "lodash-es": "4.17.21",
    "posthog-js": "1.57.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.4",
    "react-use": "17.4.0",
    "use-between": "1.3.4",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "4.17.6",
    "@types/node": "18.11.11",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "@typescript-eslint/parser": "5.45.1",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "10.4.13",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "postcss": "8.4.19",
    "sass": "1.57.1",
    "typescript": "4.6.4",
    "vite": "^4.4.7",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-svgr": "^3.2.0"
  }
}
